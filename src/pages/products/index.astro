---
import PageHeader from "@components/PageHeader.astro";
import Shape from "@components/Shape.astro";
import Base from "@layouts/Base.astro";
import Faq from "@layouts/function-components/Faq.jsx";
import PricingCard from "@layouts/function-components/PricingCard.jsx";
import { getEntryBySlug } from "astro:content";
import ProductsExperiences from "@components/products/ProductsExperiences.astro"
import products_data from '../../content/products/data/products.json';

const pricing = await getEntryBySlug("products", "index");
console.log(pricing)
const page_data = {
  ...pricing.data,
  content: pricing.body,
};
console.log(page_data)
---

<Base
  title={pricing.data.title}
  meta_title={pricing.data.meta_title}
  description={pricing.data.description}
  image={pricing.data.image}
>
  <Shape />
  <section class="page-hero pb-4 pt-16">
    <div class="container">
      <PageHeader page_data={page_data} />
    </div>
  </section>
  <!-- pricing -->
  <section class="section mt-12 pt-0">
    <ProductsExperiences/>
  </section>
  <Faq client:load data={pricing.data} />
</Base>